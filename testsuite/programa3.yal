module programa3 {

function a[]=f1(b[]) {
		 i = 0;
		 N = b.size;
		 a = [N];
		 while(i<b.size) {
		 	a[i] = b[i];
		 	i = i +1;
		 }
}

function a[]=f2(N) {
		 a = [N];
		 i=0;
		 while(i<a.size)
		 {
			a[i]=1;
			i = i + 1;
		 }
}

function main() {
	b = [100];
	b[0] = 1;
	b[99] = 2;

	a = f1(b);

	first = a[0];
	last = a[99];
	io.println("first: ",first);
	io.println("last: ",last);

	a = f2(100);

	first = a[0];
	last = a[99];
	io.println("first: ",first);
	io.println("last: ",last);
}
}

