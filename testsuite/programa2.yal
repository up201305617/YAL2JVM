module programa2 {

function a[]=f1(b[]) {
		 i = 0;
		 N = b.size;
		 a = [N];
		 while(i<b.size) {
		 	a[i] = b[i];
		 	i = i +1;
		 }
}

function main() {
	b = [100];
	b[0] = 1;
	b[99] = 2;

	a = f1(b);

	first = a[0];
	last = a[99];
	io.println("first: ",first);
	io.println("last: ",last);

}
}

